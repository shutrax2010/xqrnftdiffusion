<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="icon" type="image/x-icon" href="../assets/favicon.ico">
  <link rel="stylesheet" href="/stylesheets/NftShop.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
  <title>NFT Shop</title>
</head>
<body>

<header>
  <div class="header-content">
    <a href="/nftShopHome">
      <img src="../assets/PhygitalifyQR icon.png" alt="„Ç¢„Ç§„Ç≥„É≥" class="top-icon"/>
    </a>
    <div class="PhygitalifhQR">PhygitalifhQR</div>
    <div class="search-wrapper">
      <input type="text" class="search-bar" placeholder="Search event(Dummy function)">
      <a class="search-button" aria-label="Ê§úÁ¥¢">üîç</a>
    </div>
    <div class="account-name">John Doe</div> <!--‰ªÆ„ÅÆ„ÇÇ„ÅÆ-->
    <a href="/nftView" class="button">My tickets</a>
  </div>
</header>

<div class="header-image">
  <img src="../assets/viewer-app6.png" alt="Header Image">
  <div class="header-overlay">
  </div>
</div>

<div class="header-text">
  <p>Summer Music Festival 2024</p>
</div>

<div id="ipfs-image" class="nft-list">
  <% if (nfts && nfts.length > 0) { %>
    <% nfts.forEach(nft => { %>
      <% if (nft.imageUrl) { %>
        <div class="nft-item">
          <img src="<%= nft.imageUrl %>" alt="<%= nft.name %>" class="nft-image">
          <% if (nft.NFTokenTaxon !== undefined) { %>
            <p>NFTokenTaxon: <%= nft.NFTokenTaxon %></p>
          <% } else { %>
            <p>NFTokenTaxon: Not available</p>
          <% } %>
          <a href="#" class="button">Check out</a>
        </div>
      <% } %>
    <% }); %>
  <% } else { %>
    <p>No images found.</p>
  <% } %>
</div>

<footer>
  <div class="footer-content">
    <p>&copy; 2024 Event Tickets Center. All rights reserved.</p>
    <div class="social-icons">
      <a href="https://facebook.com" target="_blank" class="social-icon facebook" aria-label="Facebook">
        <i class="fab fa-facebook-f"></i>
      </a>
      <a href="https://instagram.com" target="_blank" class="social-icon instagram" aria-label="Instagram">
        <i class="fab fa-instagram"></i>
      </a>
      <a href="https://twitter.com" target="_blank" class="social-icon twitter" aria-label="Twitter">
        <i class="fab fa-twitter"></i>
      </a>
    </div>
  </div>
</footer>

</body>
</html>
